<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="MerchanServ.View.MenuSuperv">
    <ContentPage.Content>

        <StackLayout Margin="50,10" Padding="30,20" Orientation="Vertical">
            <Button Clicked="Button_AddMerc" BackgroundColor="Black" TextColor="White"  Text="Add Merchandiser" />
            <Button Clicked="Button_AddStore" BackgroundColor="Black" TextColor="White"  Text="Add Store" />
            <Button Clicked="Button_AddTicket" BackgroundColor="Black" TextColor="White"  Text="Add Ticket" />
            <Button Clicked="Button_ListTicket" BackgroundColor="Black" TextColor="White"  Text="List Tickets" />
            <Button Clicked="Button_ListMerc" BackgroundColor="Black" TextColor="White"  Text="List Merchandisers" />
        </StackLayout>
        
    </ContentPage.Content>
</ContentPage>
